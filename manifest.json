{
    "name": "YouTube　AdSkip",
    "manifest_version": 2,
    "version": "1.0.0.0",
    "description":"YouTubeの広告のスキップボタンを自動で押すよ！！！しかもなんか5秒待たなくてもよくなってるよ！！！",
    "browser_action": {
      "default_title": "YouTube　AdSkip",
      "default_icon": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content_scripts.js"]
      }
    ],

    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
}
  